{
    "name": "will-taco/composer-edd-plugin",
    "description": "Composer plugin for installing pro plugins powered by EDD software licensing",
    "type": "composer-plugin",
    "require": {
        "composer-plugin-api": "^2.0",
        "vlucas/phpdotenv": "^5.0.0"
    },
    "require-dev": {
        "composer/composer": "^2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ajit Bohra",
            "email": "ajit@lubus.in"
        }
    ],
    "autoload": {
        "psr-4": {
            "will-taco\\ComposerEddPlugin\\": "src"
        }
    },
    "extra": {
        "class": "will-taco\\ComposerEddPlugin\\Plugin",
        "plugin-modifies-downloads": true
    }
}
